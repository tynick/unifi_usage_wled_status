{
	"system": {
		"task-scheduler": {
			"task": {
				"get_usage": {
					"executable": {
						"path": "/config/scripts/get_usage.sh"
					},
					"interval": "1m"
				},
				"reset_usage": {
					"executable": {
						"path": "/config/scripts/reset_usage.sh"
					},
					"crontab-spec": "1 0 26 * *"
				}
			}
		}
	}
}
